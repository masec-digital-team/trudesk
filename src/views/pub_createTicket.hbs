            function validateEmail(email) {
                var mailFormat = /^\w+([.-]\w+)*@\w+([.-]\w+)*(\.\w+)+$/;
                return email.match(mailFormat);
            }
            function validateGST(gst) {
                var gstFormat = /^([0-9]{2}[a-zA-Z]{4}([a-zA-Z]{1}|[0-9]{1})[0-9]{4}[a-zA-Z]{1}([a-zA-Z]|[0-9]){3}){0,15}$/;
                return gst.match(gstFormat);
            }
            function validateMobile(mobile) {
                var mobileFormat = /[2-9]{2}\d{8}/;
                return mobile.match(mobileFormat);
            }
            function validatePincode(pincode) {
                var pincodeFormat = /^[1-9][0-9]{5}$/gm;
                return pincode.match(pincodeFormat);
            }

            $.fn.serializeObject = function(){
                var obj = {};

                $.each( this.serializeArray(), function(i,o){
                    var n = o.name,
                            v = o.value;

                    obj[n] = obj[n] === undefined ? v
                            : $.isArray( obj[n] ) ? obj[n].concat( v )
                            : [ obj[n], v ];
                });

                return obj;
            };


        });
    });
